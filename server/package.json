{
  "name": "deedchain-server",
  "version": "1.0.0",
  "description": "DeedChain Backend API - Land Ownership & Tokenization Platform",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "build": "npm run db:generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "db:generate": "npx prisma generate --schema ../database/prisma/schema.prisma",
    "db:migrate": "npx prisma migrate dev --schema ../database/prisma/schema.prisma",
    "db:reset": "npx prisma migrate reset --schema ../database/prisma/schema.prisma",
    "db:seed": "node src/scripts/seed.js",
    "db:studio": "npx prisma studio",
    "docker:build": "docker build -t deedchain-server .",
    "docker:run": "docker run -p 5000:5000 deedchain-server"
  },
  "keywords": [
    "blockchain",
    "land-registry",
    "nft",
    "property",
    "tokenization",
    "web3"
  ],
  "author": "DeedChain Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "@prisma/client": "^5.2.0",
    "web3.storage": "^4.4.0",
    "ethers": "^6.7.0",
    "redis": "^4.6.7",
    "bull": "^4.11.3",
    "nodemailer": "^6.9.4",
    "joi": "^17.9.2",
    "multer": "^1.4.5-lts.1",
    "morgan": "^1.10.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.2.0",
    "jest": "^29.6.2",
    "eslint": "^8.48.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deedchain/backend"
  }
}